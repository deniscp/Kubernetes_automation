<network>
  <name>{{ net_domain_name }}</name>
  <forward mode="nat">
    <nat>
      <port start="1024" end="65535"/>
    </nat>
  </forward>
  <domain name="{{ net_domain_name }}"/>
  <ip address="{{ net_gateway_IP }}" netmask="{{ net_netmask }}">
    <dhcp>
      <range start="{{ net_dhcp_start }}" end="{{ net_dhcp_end }}"/>
    </dhcp>
  </ip>
</network>
